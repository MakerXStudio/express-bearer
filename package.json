{
  "name": "@makerx/express-bearer",
  "version": "1.2.0",
  "private": false,
  "description": "Bearer token validation middleware for Express.js",
  "author": "MakerX",
  "license": "MIT",
  "type": "module",
  "module": "index.mjs",
  "types": "index.d.ts",
  "main": "index.cjs",
  "engines": {
    "node": ">=16.0"
  },
  "bugs": {
    "url": "https://github.com/MakerXStudio/express-bearer/issues"
  },
  "homepage": "https://github.com/MakerXStudio/express-bearer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MakerXStudio/express-bearer.git"
  },
  "scripts": {
    "audit": "better-npm-audit audit",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "check-types": "tsc -p tsconfig.json",
    "build": "run-s build:*",
    "build:0-clean": "rimraf dist coverage",
    "build:1-lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "build:2-check-types": "tsc -p tsconfig.json",
    "build:3-build": "rollup -c --configPlugin typescript",
    "build:4-copy-pkg-json": "tstk copy-package-json -c",
    "build:5-copy-readme": "copyfiles ./README.md ./dist",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter junit --outputFile test-results.xml"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@makerx/eslint-config": "^4.2.0",
    "@makerx/node-common": "^1.5.0",
    "@makerx/prettier-config": "^2.0.1",
    "@makerx/ts-toolkit": "^4.0.0-beta.24",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/node20": "20.1.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.16",
    "@vitest/coverage-v8": "^3.1.1",
    "better-npm-audit": "^3.11.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.38.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "express": "*"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0"
  }
}
